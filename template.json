                {
                    "parameter": "$F{tegevusalaTapsustus}.getNimetus()",
                    "title": "\"Tegevusala täpsustus\"",
                    "check": "$F{tegevusalaTapsustus} != null"
                },
                {
                    "parameter": "$F{emtak}.getNimetus() + \" \" + $F{emtak}.getKood()",
                    "title": "\"Klassifikaatori kohane nimetus/EMTAK\"",
                    "check": "$F{emtak} != null"
                },
                {
                    "parameter": "$F{maksimumvoimsus}",
                    "title": "\"Maksimumvõimsus\"",

{
    "tegevusalad": [
        {
            "__comment__": "Loomade karantiniseerimine",
            "type": "LOAGA_KARANTINISEERIMINE",
            "frames": [
                {
                    "parameter": "$F{tegevuseYldnimetus}.getNimetus()",
                    "title": "\"Tegevuse üldnimetus\"",
                    "check": "$F{tegevuseYldnimetus} != null"
                },
                {
                    "parameter": "$F{loomaliik}.getNimetus()",
                    "title": "\"Loomaliik\"",
                    "check": "$F{loomaliik} != null"
                },
                {
                    "parameter": "$F{tegevusalaNimetus}.getNimetus()",
                    "title": "\"Tegevusala nimetus\"",
                    "check": "$F{tegevusalaNimetus} != null"
